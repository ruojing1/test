{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Formatter",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/formatter-approuter/node_modules/@sap/html5-repo-mock/index.js",
      "args": [
        "--standalone",
        "/trainingbiUI5Module/index.html",
        "--ui5",
        "{\"version\":\"1.71.31\"}"
      ],
      "cwd": "${workspaceFolder}/UI5Module",
      "env": {
        "PORT": "6004",
        "MOCK_LOOKUP_DIRS": "[\"webapp\"]",
        "run.config": "{\"handlerId\":\"ui5_run_config_handler_id\",\"runnableId\":\"/home/user/projects/Formatter/UI5Module\"}"
      },
      "envFile": "${workspaceFolder}/UI5Module/.env2",
      "preLaunchTask": "Build Formatter"
    }
  ]
}